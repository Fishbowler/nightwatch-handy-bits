{
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Launch with Firefox",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}\\node_modules\\nightwatch\\bin\\runner.js",
            "stopOnEntry": false,
            "args": ["--config", "${workspaceRoot}\\nightwatch.json", "--test", "${file}", "-e", "firefox"],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "externalConsole": true,
            "sourceMaps": false,
            "outFiles": []
        },
